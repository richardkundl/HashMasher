@model IEnumerable<HashMasher.Model.ProcessedLink>
@{
    ViewBag.Title = "Home Page";
}
<div class="page-header">
    <h1>Tweets <small>from conferences.....with links!</small></h1>
</div>


<div class="row">
    <div class="span3">
        <h2>left</h2>
    </div>
    <div class="span11">
        <h3>Right</h3>
        @foreach (var item in Model)
        {
                @Html.Partial("_Tweet", item)
        }
    </div>
</div>

